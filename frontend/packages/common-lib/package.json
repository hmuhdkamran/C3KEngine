{
  "name": "common-lib",
  "version": "0.0.1",  
  "type": "module",
  "files": ["dist"],
  "main": "./dist/common-lib.umd.cjs",
  "module": "./dist/common-lib.js",
  "exports": {
    ".": {
      "import": "./dist/common-lib.js",
      "require": "./dist/common-lib.umd.cjs"
    },
    "./style.css": "./dist/style.css"
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build-lib": "vite build && vue-tsc --emitDeclarationOnly",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force --emitDeclarationOnly",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "pinia": "^2.2.2",
    "vue": "^3.5.8"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.250",
    "@iconify/tailwind": "^1.1.3",
    "@rushstack/eslint-patch": "^1.8.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^22.5.5",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^9.11.0",
    "eslint-plugin-vue": "^9.23.0",
    "npm-run-all2": "^6.2.3",
    "prettier": "^3.3.3",
    "dotenv": "^16.4.5",
    "postcss": "^8.4.47",
    "typescript": "^5.6.2",
    "vite": "^5.4.7",
    "vite-plugin-vue-devtools": "^7.4.5",
    "vue-tsc": "^2.1.6"
  }
}
